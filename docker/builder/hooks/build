#!/bin/bash

echo "Build.Username"
echo $ARG_COMPOSER_MAGENTO_USERNAME
echo "Build.Password"
echo $ARG_COMPOSER_MAGENTO_PASSWORD
echo "Build.Imagename"
echo $IMAGE_NAME

echo "WHAMI"
whoami

docker build --build-arg ARG_COMPOSER_MAGENTO_PASSWORD=$ARG_COMPOSER_MAGENTO_PASSWORD --build-arg ARG_COMPOSER_MAGENTO_USERNAME=$ARG_COMPOSER_MAGENTO_USERNAME -f $DOCKERFILE_PATH -t $IMAGE_NAME .
