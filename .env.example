COMPOSE_PROJECT_NAME=magento24_test

MYSQL_HOSTNAME=db
MYSQL_ROOT_PASSWORD=root
MYSQL_USER=user
MYSQL_PASSWORD=root
MYSQL_DATABASE=magento
CRYPTO_KEY=secured

VIRTUAL_HOST=magento24.test
VIRTUAL_PORT=443
SERVER_SSL=on
USER_HOME=/var/www/magento
SERVER_ROOT=/var/www/magento/src
MAGENTO_RUN_MODE=developer
MAGENTO_BASE_URL=https://magento24.test/
MAGENTO_BACKEND_FRONTNAME=manager
MAGENTO_ADMIN_FIRSTNAME=Admin
