MAGENTO_ADMIN_PASSWORD=a123456
MAGENTO_LANGUAGE=en_US
MAGENTO_CURRENCY=USD
MAGENTO_TIMEZONE=Europe/Moscow

PHP_MEMORY_LIMIT=2G
PHP_IDE_CONFIG="serverName=local.test"
XDEBUG_MODE=off
UPLOAD_MAX_FILESIZE=64M
DEBUG=false

